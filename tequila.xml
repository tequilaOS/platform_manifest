<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="tequila"
           fetch="https://github.com/tequilaOS"
           revision="refs/heads/taco"
           review="https://review.tequilaos.org" />

  <remote  name="tequila-gitlab"
           fetch="https://gitlab.com/tequilaOS/android"
           revision="refs/heads/taco"
           review="https://review.tequilaos.org" />

  <remote  name="github"
           fetch="https://github.com" />

  <!-- rootdir -->
  <remove-project name="platform/bionic" />
  <project path="bionic" name="platform_bionic" remote="tequila" />
